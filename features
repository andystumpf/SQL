SELECT func,
       'https://admin.hudl.com/user/' + authuser::varchar AS authuser,
       'https://admin.hudl.com/team/' + acc::varchar AS account,
       SUBSTRING(REGEXP_SUBSTR(attributes, 'Feature=[0-9]*'), 9) AS feature,
       messagetime,
       message
FROM logs
WHERE func IN ('Insert',
               'Delete')
  AND op='Feature'
  AND messagetime >= GETDATE()-7
  AND feature IN ({{ priv }})
  AND authuser NOT IN
    (SELECT adminid
     FROM roster
     WHERE end_date IS NULL)
